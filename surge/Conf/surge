[Script]

[MITM]
# > 跳过服务端证书验证
skip-server-cert-verify = true

# > 用于 TCP 连接
tcp-connection = true

# > MITM over HTTP/2
h2 = true

ca-passphrase = 21641934
ca-p12 = 
